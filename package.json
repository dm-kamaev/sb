{
  "name": "sber-together-api",
  "version": "1.0.0",
  "description": "",
  "main": "app/app.js",
  "scripts": {
    "migrate": "cd node_modules/user-service && ant -f build.xml user.migrate -Duser.sequelize=../.bin/sequelize -Duser.config-path=../../config/user-config && cd ../.. && node app/scripts/migrate.js",
    "config": "node app/scripts/createConfig.js",
    "start-users": "node app/scripts/startUserService.js",
    "start-auth": "node app/scripts/startAuthService.js",
    "lint-fix": "node_modules/.bin/eslint app/** --fix --config ./node_modules/nodules/.eslintrc || true",
    "drop-all": "psql -d sber-together-api -c \"DROP SCHEMA IF EXISTS \"public\" CASCADE;CREATE SCHEMA \"public\";\"",
    "update": "node node_modules/nodules/npmUpdate",
    "start": "node app/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/c7s/sber-together-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/c7s/sber-together-api/issues"
  },
  "homepage": "https://github.com/c7s/sber-together-api#readme",
  "dependencies": {
    "asyncawait": "^1.0.3",
    "auth-service": "git+ssh://git@github.com:c7s/auth-service.git#dev",
    "axios": "^0.12.0",
    "body-parser": "^1.15.1",
    "connect-session-sequelize": "^3.1.0",
    "continuation-local-storage": "^3.1.7",
    "cookie-signature": "^1.0.6",
    "deployment": "git+ssh://git@github.com:c7s/deployment.git#dev",
    "express": "^4.13.4",
    "express-session": "^1.14.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "morgan": "^1.7.0",
    "nodules": "git+ssh://git@github.com/c7s/nodules.git#dev",
    "passport": "^0.3.2",
    "pg": "^5.1.0",
    "pg-hstore": "^2.3.2",
    "pug": "^0.1.0",
    "q": "^1.4.1",
    "sber-together": "git+ssh://git@github.com:c7s/sber-together.git#dev",
    "sequelize": "^3.23.3",
    "sequelize-cli": "^1.9.1",
    "user-service": "git+ssh://git@github.com/c7s/user-service.git#dev"
  },
  "devDependencies": {
    "gulp-apidoc": "^0.2.4"
  }
